    {
        Account firm=new Account(Name='Firm Account',MarsFirmId__c=123455,MarsOfficeId__c=12,RecordTypeId=MarsUtility.AccountRecordType);
        MarsUtility.BYPASS_ALL_TRIGGER=true;
        insert firm;
        MarsUtility.BYPASS_ALL_TRIGGER=false;
